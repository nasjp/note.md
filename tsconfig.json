{
   "compilerOptions":{
      "module":"es2015",
      "outDir":"dist",
      "target":"es5",
      "lib":[
         "dom",
         "es2017"
      ],
      "noImplicitAny":true,
      "strict":true,
      "strictNullChecks":true,
      "removeComments":true,
